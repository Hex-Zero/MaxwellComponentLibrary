{
  "name": "maxwellcomponentlibrary",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "powershell -NoProfile -Command Remove-Item -Recurse -Force dist -ErrorAction SilentlyContinue",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "start": "storybook dev -p 6006",
    "build": "storybook build",
    "build:local": "storybook build && pnpm postbuild:autocommit",
    "postbuild:autocommit": "git add . && git diff --cached --quiet && echo No changes to commit || (git commit -m \"chore: auto commit after successful build\" && git push origin HEAD)",
    "storybook:build-static": "rimraf storybook-static && storybook build",
    "storybook:serve-static": "npx http-server storybook-static -p 6006",
    "storybook:test-static": "pnpm run storybook:build-static && start-server-and-test storybook:serve-static http://localhost:6006 test:stories:runner",
    "test:stories": "start-server-and-test storybook http://localhost:6006 test:stories:runner",
    "test:stories:runner": "test-storybook --ci --url http://localhost:6006",
    "test:stories:watch": "test-storybook --watch",
  "test": "pnpm run lint && pnpm run type-check && jest && pnpm run test:stories:runner"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-test": "0.0.0-pr-30727-sha-fe615277",
    "@storybook/addon-themes": "^9.1.2",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.20.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "storybook": "^8.4.7",
    "style-loader": "^4.0.0",
